num = int(input('Digite um numero: '))


if num % 3 == 0 is num % 5 == 0:
        print('O numero e divisivel por 3 e 5')
elif num % 3 == 0:
       print('O numero e divisivel por 3')
elif num % 5 == 0:
        print('O numero e divisivel por 5')
else:
       print('O numero não é divisivel por 3 e 5')
